{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Run mingw32-make -f Makefile_txt",
			"command": "C:/MinGW/bin/mingw32-make",
			"args": [
				"-f",
				"Makefile_txt",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}